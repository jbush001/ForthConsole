variable ship_pos


: init
  56 ship_pos !
;


: draw_frame
    buttons BUTTON_L and if
        ship_pos @ 0 > if
            ship_pos @ 1 - ship_pos !
        then
    then

    buttons BUTTON_R and if
        ship_pos @ SCREEN_WIDTH 16 - < if
            ship_pos @ 1 + ship_pos !
        then
    then

    0 cls
    ship_pos @ 112 2 2 0 draw_sprite
;

